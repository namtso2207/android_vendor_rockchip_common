<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
    xmlns:android="http://schemas.android.com/apk/res/android">

    <PreferenceScreen
        android:title="@string/Display_Setting"
        android:iconSpaceReserved="false">
        <intent
            android:targetClass="com.namtso.ksettings.Display_Preference"
            android:targetPackage="com.namtso.ksettings" />
    </PreferenceScreen>
    <PreferenceScreen
        android:title="@string/LEDs"
        android:iconSpaceReserved="false">
        <intent
            android:targetClass="com.namtso.ksettings.LEDs_Preference"
            android:targetPackage="com.namtso.ksettings" />
    </PreferenceScreen>

    <ListPreference
        android:key="FAN_KEY"
        android:iconSpaceReserved="false"
        android:title="@string/Cooling_fan"
        android:summary="@string/FAN_default"
        android:defaultValue="1"
        android:dialogTitle="@string/Cooling_fan"
        android:entries="@array/FAN_array"
        android:entryValues="@array/FAN_array_value">
    </ListPreference>

    <SwitchPreference
        android:key="WOL_KEY"
        android:iconSpaceReserved="false"
        android:title="@string/WOL"
		android:summaryOn="@string/WOL_ON_summary"
        android:summaryOff="@string/WOL_OFF_summary"/>
    <SwitchPreference
        android:key="EXT_WOL_KEY"
        android:iconSpaceReserved="false"
        android:title="@string/EXT_WOL"
		android:summaryOn="@string/EXT_WOL_ON_summary"
        android:summaryOff="@string/EXT_WOL_OFF_summary"/>
    <PreferenceScreen
        android:title="@string/Status_bar_control"
        android:iconSpaceReserved="false">
        <intent
            android:targetClass="com.namtso.ksettings.Status_bar_Preference"
            android:targetPackage="com.namtso.ksettings" />
    </PreferenceScreen>

    <PreferenceScreen
        android:title="@string/Timer"
        android:iconSpaceReserved="false">
        <intent
            android:targetClass="com.namtso.schpwronoff.AlarmClock"
            android:targetPackage="com.namtso.schpwronoff" />
    </PreferenceScreen>

</PreferenceScreen>